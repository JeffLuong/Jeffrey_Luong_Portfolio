{
  "plugins": [
    ["@babel/plugin-transform-react-jsx", { "pragma": "h" }],
    "@babel/plugin-proposal-class-properties",
    ["css-modules-transform", {
      "extensions": [".css", ".scss"]
    }]
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "env": {
    "test": {
      "presets": [
        ["preact-cli/babel", { "modules": "commonjs" }]
      ]
    }
  }
}